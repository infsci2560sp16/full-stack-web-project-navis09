<script>
	function setXML(){
		document.getElementById("content").innerHTML=
			"<a href='http://localhost:8080/hello/rest/BookResource/XML'>XML</a>"
	}
	function setJSON(){
		document.getElementById("content").innerHTML=
			"<a href='http://localhost:8080/hello/rest/BookResource/JSON'>JSON</a>"
	}
</script>

</head>
<body>
<h1>Management Homepage</h1>
	
	
	<button onclick = "setXML()">XML</button>
	<button onclick = "setJSON()">JSON</button>
	<p id="content">
    </p>
    <p id="other">
    <a href="http://localhost:8080/hello/rest/BookResource/XML">XML Link</a>
